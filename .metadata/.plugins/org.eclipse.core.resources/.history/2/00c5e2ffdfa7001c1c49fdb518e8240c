package Uebung05;

public class BruchTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stu
		Bruch b = new Bruch();
		if (true) {
			Bruch x= new Bruch();
			x.zaeler=1;
			x.nenner=4;
			b.zaeler=1;
			b.nenner=4;
			System.out.println(x.zaeler);
			System.out.println(x.nenner);
			if(x==b) {
				System.out.println(b.zaeler);
				System.out.println(b.nenner);
			}
			
		}
		
	}

}
